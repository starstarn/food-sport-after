<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bs.grad.mapper.FoodListMapper">
    <!--数据库查询语句
    id是接口名字 resultType是实体类的路径
    -->
    <select id="getById" resultType="com.bs.grad.domain.FoodList">
        select * from food_list where id = #{id}
    </select>

</mapper>